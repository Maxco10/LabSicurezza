<%= form_for(@feedback) do |f| %>
  <% if @feedback.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@feedback.errors.count, "error") %> prohibited this feedback from being saved:</h2>

      <ul>
      <% @feedback.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>
  <div class="field">
    <%= f.label :voto %><br>
    <%= f.select :voto, ['1','2','3','4','5'], include_blank: false %>
    
  </div><p>stai lasiando il feedback per l utente <%= User.where("id = ?",params[:utente]).take.nome %></p>
  <%= f.hidden_field :proprietario_id, :value => params[:utente] %>
  <%= f.hidden_field :annuncio_id, :value => params[:annuncio] %>
  <%= f.hidden_field :booking_id, :value => params[:booking] %>
  <div class="actions">
    <%= f.submit  %>
  </div>
<% end %>
