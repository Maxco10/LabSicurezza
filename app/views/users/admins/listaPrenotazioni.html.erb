<center><div class="page-header">
  <h1>Lista prenotazioni di SmartBucket</h1>
  <% if notice %>
    <div class="alert alert-success">
      <strong><%= notice %></strong>
    </div>
  <% end %>
</div></center>	

<section class="main-content">				
	<div class="row">
		<div class="span12">
		  <div class="table-responsive">					
			  <table class="table table-striped">
  				<thead>
  					<tr>
              <th>ID</th>
              <th>Titolo</th>
              <th>Proprietario</th>
              <th>Prenotato da</th>
              <th>Creato il</th>
            </tr>
  				</thead>
  				<tbody>
  					  <% @bookings.each do |bookings| %>
                <tr>
                  <td><%= bookings.id %></td>
                  <td><%= bookings.titolo_annuncio %></td>
                  <td><%= bookings.nome_proprietario %></td>
                  <td><%= bookings.nome_prenotato %></td>
                  <td><%= bookings.created_at %></td>
                </tr>
                <%end%>
    					  <tr>
                  <td>&nbsp;</td>
                  <td>&nbsp;</td>
                  <td>&nbsp;</td>
                  <td>&nbsp;</td>
                  <td>&nbsp;</td>
                </tr>		  
  					</tbody>
        </table>
      </div>
      <hr>
				<div class="pagination pagination-small pagination-centered">
					<%= will_paginate @bookings, :previous_label => 'Prec', :next_label => 'Succ', renderer: BootstrapPagination::Rails %>
				</div>
    </div>
  </div>
</section>

